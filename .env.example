# API Server Configuration
PORT=3000
NODE_ENV=development

# Authentication
# Comma-separated list of valid API tokens
# Generate secure tokens: openssl rand -hex 32
API_TOKENS=ptEKDCXxio1SuGueqn5X3uovf6ELzZAU,another-token-here

# Rate Limiting (optional - can be overridden in requests)
MIN_DELAY_BETWEEN_REQUESTS=3000
MAX_DELAY_BETWEEN_REQUESTS=8000

# Scraper Configuration
MAX_LISTINGS_PER_REQUEST=100
DEFAULT_NUMBER_OF_LISTINGS=10

# Logging
LOG_LEVEL=info
